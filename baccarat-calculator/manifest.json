{
    "manifest_version": 3,
    "name": "Baccarat Calculator",
    "description": "Simple extension that helps users to calculate odds for Baccarat games.",
    "version": "1.3",
    "action": {
        "default_title": "Baccarat Calculator",
        "default_popup": "index.html",
        "default_icon": {
            "16": "images/icon-16.png",
            "32": "images/icon-32.png",
            "48": "images/icon-48.png",
            "128": "images/icon-128.png",
            "512": "images/icon-512.png"
          }
    },
    "browser_specific_settings": {
      "gecko": {
        "id": "help@baccarat.wiki",
        "strict_min_version": "91.0"
      }
    },
    "host_permissions": [
        "<all_urls>"
    ],
    "permissions": [
        ""
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png",
        "512": "images/icon-512.png"
    }
}